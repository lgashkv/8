 class Designer(Employee):
    def __init__(self, name, seniority, awards):
        super().__init__(name, seniority)
        self.awards= awards; 
    
    def check_if_it_is_time_for_upgrade(self): 
        self.seniority +=1
        
        if self.awards  == 2:
            self.seniority +=2
            
        if self.seniority % 7 == 0:
            self.grade_up()
            
            
