import requests
def val():
    valuta = []
    nameval = requests.get('https://www.cbr-xml-daily.ru/daily_json.js').json()
    val = nameval ['Valute']
    for k,v in val.items():
        valuta.append([k,round(float(v.get('Value'))/float(v.get('Nominal')),2)])
    return max(valuta, key=lambda x: x[1])
    
val() 

